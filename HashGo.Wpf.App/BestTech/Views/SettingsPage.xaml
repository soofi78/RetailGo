<baseControls:BasePage
    x:Class="HashGo.Wpf.App.BestTech.Views.SettingsPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:baseControls="clr-namespace:HashGo.Wpf.App.BestTech.Controls"
    xmlns:behaviours="clr-namespace:HashGo.Wpf.App.Behavior"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:i="http://schemas.microsoft.com/xaml/behaviors"
    xmlns:ia="clr-namespace:Avalonia.Xaml.Interactions.Core;assembly=Avalonia.Xaml.Interactions"
    xmlns:local="clr-namespace:HashGo.Wpf.App.BestTech.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:sys="clr-namespace:System;assembly=System.Runtime"
    Title="SettingsPage"
    mc:Ignorable="d">
    <baseControls:BasePage.Resources>
        <Style TargetType="TabItem">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="TabItem">
                        <Border
                            Name="Border"
                            Margin="2,2,0,0"
                            Background="LightGray"
                            BorderBrush="Gray"
                            BorderThickness="1"
                            CornerRadius="2">
                            <ContentPresenter
                                x:Name="ContentSite"
                                Margin="10,2"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                ContentSource="Header" />
                        </Border>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsSelected" Value="True">
                                <Setter TargetName="Border" Property="Background" Value="#00B5CB" />
                            </Trigger>
                            <Trigger Property="IsEnabled" Value="False">
                                <Setter Property="Foreground" Value="Gray" />
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
    </baseControls:BasePage.Resources>

    <Grid
        HorizontalAlignment="Stretch"
        VerticalAlignment="Stretch"
        Background="White">
        <Grid.RowDefinitions>
            <RowDefinition Height="0.06*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="0.1*" />
        </Grid.RowDefinitions>

        <Grid
            Grid.Row="0"
            Margin="10,20,0,0"
            Background="Transparent">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="0.1*" />
                <ColumnDefinition />
            </Grid.ColumnDefinitions>

            <Grid Cursor="Hand">
                <i:Interaction.Triggers>
                    <i:EventTrigger EventName="PreviewMouseLeftButtonUp">
                        <i:InvokeCommandAction Command="{Binding PreviousScreenCommand}" />
                    </i:EventTrigger>
                </i:Interaction.Triggers>
                <Path
                    Cursor="Hand"
                    Data="M 10,0 L 0,10 L 10,20 M 0,10 L 20,10"
                    Stroke="Black"
                    StrokeThickness="1" />

            </Grid>

        </Grid>

        <TabControl
            Grid.Row="1"
            Margin="0,10,10,0"
            SelectedIndex="{Binding SelectedTabIndex, Mode=TwoWay}">
            <TabItem
                Width="120"
                Height="40"
                Header="Authenticate">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="0.6*" />
                        <RowDefinition Height="0.2*" />
                    </Grid.RowDefinitions>

                    <Grid
                        Grid.Row="0"
                        Margin="0,10,10,0"
                        DataContext="{Binding ConnectItem}">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition />
                            <ColumnDefinition />
                        </Grid.ColumnDefinitions>

                        <Grid.RowDefinitions>
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                        </Grid.RowDefinitions>

                        <TextBlock
                            Grid.Column="0"
                            Margin="25,0,0,0"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Center"
                            Panel.ZIndex="1000"
                            FontWeight="Bold"
                            Foreground="White"
                            Text="URL" />
                        <Border
                            Grid.Column="0"
                            Grid.ColumnSpan="2"
                            Background="#8C97C4"
                            BorderBrush="#8C97C4"
                            BorderThickness="1"
                            CornerRadius="9" />
                        <Border
                            Grid.Row="0"
                            Grid.Column="1"
                            Margin="2,12,10,10"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            CornerRadius="12">
                            <TextBox
                                VerticalContentAlignment="Stretch"
                                Background="White"
                                BorderThickness="0"
                                Foreground="Black"
                                Text="{Binding Url, UpdateSourceTrigger=PropertyChanged}">
                                <i:Interaction.Behaviors>
                                    <behaviours:TextBoxEnterNavigateBahaviour />
                                </i:Interaction.Behaviors>
                            </TextBox>
                        </Border>

                        <TextBlock
                            Grid.Row="1"
                            Grid.Column="0"
                            Margin="25,0,0,0"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Center"
                            Panel.ZIndex="1000"
                            FontWeight="Bold"
                            Foreground="White"
                            Text="Tenant Name" />
                        <Border
                            Grid.Row="1"
                            Grid.Column="0"
                            Grid.ColumnSpan="2"
                            Margin="0,5,0,0"
                            Background="#8C97C4"
                            BorderBrush="#8C97C4"
                            BorderThickness="1"
                            CornerRadius="9" />
                        <Border
                            Grid.Row="1"
                            Grid.Column="1"
                            Margin="2,15,10,10"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            CornerRadius="12">
                            <TextBox
                                VerticalContentAlignment="Center"
                                Background="White"
                                BorderThickness="0"
                                Foreground="Black"
                                Text="{Binding TenantName, UpdateSourceTrigger=PropertyChanged}">
                                <i:Interaction.Behaviors>
                                    <behaviours:TextBoxEnterNavigateBahaviour />
                                </i:Interaction.Behaviors>
                            </TextBox>
                        </Border>

                        <TextBlock
                            Grid.Row="2"
                            Grid.Column="0"
                            Margin="25,0,0,0"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Center"
                            Panel.ZIndex="1000"
                            FontWeight="Bold"
                            Foreground="White"
                            Text="User Name" />
                        <Border
                            Grid.Row="2"
                            Grid.Column="0"
                            Grid.ColumnSpan="2"
                            Margin="0,5,0,0"
                            Background="#8C97C4"
                            BorderBrush="#8C97C4"
                            BorderThickness="1"
                            CornerRadius="9" />
                        <Border
                            Grid.Row="2"
                            Grid.Column="1"
                            Margin="2,15,10,10"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            CornerRadius="12">
                            <TextBox
                                VerticalContentAlignment="Center"
                                Background="White"
                                BorderThickness="0"
                                Foreground="Black"
                                Text="{Binding User, UpdateSourceTrigger=PropertyChanged}">
                                <i:Interaction.Behaviors>
                                    <behaviours:TextBoxEnterNavigateBahaviour />
                                </i:Interaction.Behaviors>
                            </TextBox>
                        </Border>

                        <TextBlock
                            Grid.Row="3"
                            Grid.Column="0"
                            Margin="25,0,0,0"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Center"
                            Panel.ZIndex="1000"
                            FontWeight="Bold"
                            Foreground="White"
                            Text="Password" />
                        <Border
                            Grid.Row="3"
                            Grid.Column="0"
                            Grid.ColumnSpan="2"
                            Margin="0,5,0,0"
                            Background="#8C97C4"
                            BorderBrush="#8C97C4"
                            BorderThickness="1"
                            CornerRadius="9" />
                        <Border
                            Grid.Row="3"
                            Grid.Column="1"
                            Margin="2,15,10,10"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            CornerRadius="12">
                            <TextBox
                                VerticalContentAlignment="Center"
                                Background="White"
                                BorderThickness="0"
                                Foreground="Black"
                                Text="{Binding Password, UpdateSourceTrigger=PropertyChanged}">
                                <i:Interaction.Behaviors>
                                    <behaviours:TextBoxEnterNavigateBahaviour />
                                </i:Interaction.Behaviors>
                            </TextBox>
                        </Border>

                        <TextBlock
                            Grid.Row="4"
                            Grid.Column="0"
                            Margin="25,0,0,0"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Center"
                            Panel.ZIndex="1000"
                            FontWeight="Bold"
                            Foreground="White"
                            Text="Device Id" />
                        <Border
                            Grid.Row="4"
                            Grid.Column="0"
                            Grid.ColumnSpan="2"
                            Margin="0,5,0,0"
                            Background="#8C97C4"
                            BorderBrush="#8C97C4"
                            BorderThickness="1"
                            CornerRadius="9" />
                        <Border
                            Grid.Row="4"
                            Grid.Column="1"
                            Margin="2,15,10,10"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            CornerRadius="12">
                            <TextBox
                                VerticalContentAlignment="Center"
                                Background="White"
                                BorderThickness="0"
                                Foreground="Black"
                                Text="{Binding DeviceId, UpdateSourceTrigger=PropertyChanged}">
                                <i:Interaction.Behaviors>
                                    <behaviours:IntegerInputBehaviour />
                                    <behaviours:TextBoxEnterNavigateBahaviour />
                                </i:Interaction.Behaviors>
                            </TextBox>
                        </Border>

                        <TextBlock
                            Grid.Row="5"
                            Grid.Column="0"
                            Margin="25,0,0,0"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Center"
                            Panel.ZIndex="1000"
                            FontWeight="Bold"
                            Foreground="White"
                            Text="Tenant Id" />
                        <Border
                            Grid.Row="5"
                            Grid.Column="0"
                            Grid.ColumnSpan="2"
                            Margin="0,5,0,0"
                            Background="#8C97C4"
                            BorderBrush="#8C97C4"
                            BorderThickness="1"
                            CornerRadius="9" />
                        <Border
                            Grid.Row="5"
                            Grid.Column="1"
                            Margin="2,15,10,10"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            CornerRadius="12">
                            <TextBox
                                VerticalContentAlignment="Center"
                                Background="White"
                                BorderThickness="0"
                                Foreground="Black"
                                Text="{Binding TenantId, UpdateSourceTrigger=PropertyChanged}">
                                <i:Interaction.Behaviors>
                                    <behaviours:IntegerInputBehaviour />
                                    <behaviours:TextBoxEnterNavigateBahaviour />
                                </i:Interaction.Behaviors>
                            </TextBox>
                        </Border>

                        <TextBlock
                            Grid.Row="6"
                            Grid.Column="0"
                            Margin="25,0,0,0"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Center"
                            Panel.ZIndex="1000"
                            FontWeight="Bold"
                            Foreground="White"
                            Text="Location Id" />
                        <Border
                            Grid.Row="6"
                            Grid.Column="0"
                            Grid.ColumnSpan="2"
                            Margin="0,5,0,0"
                            Background="#8C97C4"
                            BorderBrush="#8C97C4"
                            BorderThickness="1"
                            CornerRadius="9" />
                        <Border
                            Grid.Row="6"
                            Grid.Column="1"
                            Margin="2,15,10,10"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            CornerRadius="12">
                            <TextBox
                                VerticalContentAlignment="Center"
                                Background="White"
                                BorderThickness="0"
                                Foreground="Black"
                                Text="{Binding LocationId, UpdateSourceTrigger=PropertyChanged}">
                                <i:Interaction.Behaviors>
                                    <behaviours:IntegerInputBehaviour />
                                    <behaviours:TextBoxEnterNavigateBahaviour />
                                </i:Interaction.Behaviors>
                            </TextBox>
                        </Border>

                        <TextBlock
                            Grid.Row="7"
                            Grid.Column="0"
                            Margin="25,0,0,0"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Center"
                            Panel.ZIndex="1000"
                            FontWeight="Bold"
                            Foreground="White"
                            Text="Sort Order" />
                        <Border
                            Grid.Row="7"
                            Grid.Column="0"
                            Grid.ColumnSpan="2"
                            Margin="0,5,0,0"
                            Background="#8C97C4"
                            BorderBrush="#8C97C4"
                            BorderThickness="1"
                            CornerRadius="9" />
                        <Border
                            Grid.Row="7"
                            Grid.Column="1"
                            Margin="2,15,10,10"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            CornerRadius="12">
                            <TextBox
                                VerticalContentAlignment="Center"
                                Background="White"
                                BorderThickness="0"
                                Foreground="Black"
                                Text="{Binding SortOrder, UpdateSourceTrigger=PropertyChanged}">
                                <i:Interaction.Behaviors>
                                    <behaviours:IntegerInputBehaviour />
                                    <behaviours:TextBoxEnterNavigateBahaviour />
                                </i:Interaction.Behaviors>
                            </TextBox>
                        </Border>

                        <TextBlock
                            Grid.Row="8"
                            Grid.Column="0"
                            Margin="25,0,0,0"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Center"
                            Panel.ZIndex="1000"
                            FontWeight="Bold"
                            Foreground="White"
                            Text="Payments wait period(in seconds)" />
                        <Border
                            Grid.Row="8"
                            Grid.Column="0"
                            Grid.ColumnSpan="2"
                            Margin="0,5,0,0"
                            Background="#8C97C4"
                            BorderBrush="#8C97C4"
                            BorderThickness="1"
                            CornerRadius="9" />
                        <Border
                            Grid.Row="8"
                            Grid.Column="1"
                            Margin="2,15,10,10"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            CornerRadius="12">
                            <TextBox
                                VerticalContentAlignment="Center"
                                Background="White"
                                BorderThickness="0"
                                Foreground="Black"
                                Text="{Binding PaymentScreenVisibleDelay, UpdateSourceTrigger=PropertyChanged}">
                                <i:Interaction.Behaviors>
                                    <behaviours:IntegerInputBehaviour />
                                    <behaviours:TextBoxEnterNavigateBahaviour />
                                </i:Interaction.Behaviors>
                            </TextBox>
                        </Border>

                    </Grid>
                </Grid>
            </TabItem>
            <TabItem
                Width="120"
                Height="40"
                Header="Printer Settings">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition />
                        <ColumnDefinition />
                    </Grid.ColumnDefinitions>

                    <Grid.RowDefinitions>
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                    </Grid.RowDefinitions>

                    <TextBlock
                        Grid.Row="0"
                        Grid.Column="0"
                        Margin="25,0,0,0"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Center"
                        Panel.ZIndex="1000"
                        FontWeight="Bold"
                        Foreground="White"
                        Text="Set Background Image" />
                    <Border
                        Grid.Row="0"
                        Grid.Column="0"
                        Grid.ColumnSpan="2"
                        Margin="0,5,0,0"
                        Background="#8C97C4"
                        BorderBrush="#8C97C4"
                        BorderThickness="1"
                        CornerRadius="9" />
                    <Border
                        Grid.Row="0"
                        Grid.Column="1"
                        Margin="2,15,10,10"
                        HorizontalAlignment="Stretch"
                        VerticalAlignment="Stretch"
                        CornerRadius="12">
                        <TextBox
                            VerticalContentAlignment="Center"
                            Background="White"
                            BorderThickness="0"
                            Foreground="Black"
                            Text="{Binding BackgroundImage, UpdateSourceTrigger=PropertyChanged}">
                            <i:Interaction.Triggers>
                                <i:EventTrigger EventName="PreviewMouseLeftButtonUp">
                                    <i:InvokeCommandAction Command="{Binding SelectBackgroundImageCommand}" />
                                </i:EventTrigger>
                            </i:Interaction.Triggers>

                        </TextBox>
                    </Border>

                    <TextBlock
                        Grid.Row="1"
                        Grid.Column="0"
                        Margin="25,0,0,0"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Center"
                        Panel.ZIndex="1000"
                        FontWeight="Bold"
                        Foreground="White"
                        Text="Currency Symbol" />
                    <Border
                        Grid.Row="1"
                        Grid.Column="0"
                        Grid.ColumnSpan="2"
                        Margin="0,5,0,0"
                        Background="#8C97C4"
                        BorderBrush="#8C97C4"
                        BorderThickness="1"
                        CornerRadius="9" />
                    <Border
                        Grid.Row="1"
                        Grid.Column="1"
                        Margin="2,15,10,10"
                        HorizontalAlignment="Stretch"
                        VerticalAlignment="Stretch"
                        CornerRadius="12">
                        <TextBox
                            VerticalContentAlignment="Center"
                            Background="White"
                            BorderThickness="0"
                            Foreground="Black"
                            Text="{Binding CurrencySymbol, UpdateSourceTrigger=PropertyChanged}" />
                    </Border>

                    <TextBlock
                        Grid.Row="2"
                        Grid.Column="0"
                        Margin="25,0,0,0"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Center"
                        Panel.ZIndex="1000"
                        FontWeight="Bold"
                        Foreground="White"
                        Text="Menu Background Transparency" />
                    <Border
                        Grid.Row="2"
                        Grid.Column="0"
                        Grid.ColumnSpan="2"
                        Margin="0,5,0,0"
                        Background="#8C97C4"
                        BorderBrush="#8C97C4"
                        BorderThickness="1"
                        CornerRadius="9" />
                    <Border
                        Grid.Row="2"
                        Grid.Column="1"
                        Margin="2,15,10,10"
                        HorizontalAlignment="Stretch"
                        VerticalAlignment="Stretch"
                        CornerRadius="12">
                        <StackPanel
                            Margin="10"
                            VerticalAlignment="Center"
                            Orientation="Horizontal">
                            <Slider
                                x:Name="PART_Slider"
                                Width="200"
                                VerticalAlignment="Center"
                                Cursor="Hand"
                                Maximum="1"
                                Minimum="0"
                                Value="{Binding MenuBackgroundTransparency, Mode=TwoWay}" />
                            <TextBlock
                                Margin="10,0,0,0"
                                VerticalAlignment="Center"
                                FontWeight="Bold"
                                Text="{Binding Value, ElementName=PART_Slider, StringFormat=\{0:0.00\}}" />
                        </StackPanel>
                    </Border>

                    <TextBlock
                        Grid.Row="3"
                        Grid.Column="0"
                        Margin="25,0,0,0"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Center"
                        Panel.ZIndex="1000"
                        FontWeight="Bold"
                        Foreground="White"
                        Text="Show Language Selection" />
                    <Border
                        Grid.Row="3"
                        Grid.Column="0"
                        Grid.ColumnSpan="2"
                        Margin="0,5,0,0"
                        Background="#8C97C4"
                        BorderBrush="#8C97C4"
                        BorderThickness="1"
                        CornerRadius="9" />
                    <Border
                        Grid.Row="3"
                        Grid.Column="1"
                        Margin="2,15,10,10"
                        HorizontalAlignment="Stretch"
                        VerticalAlignment="Stretch"
                        CornerRadius="12">
                        <CheckBox
                            VerticalAlignment="Center"
                            FontWeight="Bold"
                            IsChecked="{Binding ShowLanguageSelection}" />
                    </Border>

                    <TextBlock
                        Grid.Row="4"
                        Grid.Column="0"
                        Margin="25,0,0,0"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Center"
                        Panel.ZIndex="1000"
                        FontWeight="Bold"
                        Foreground="White"
                        Text="Show Membership button" />
                    <Border
                        Grid.Row="4"
                        Grid.Column="0"
                        Grid.ColumnSpan="2"
                        Margin="0,5,0,0"
                        Background="#8C97C4"
                        BorderBrush="#8C97C4"
                        BorderThickness="1"
                        CornerRadius="9" />
                    <Border
                        Grid.Row="4"
                        Grid.Column="1"
                        Margin="2,15,10,10"
                        HorizontalAlignment="Stretch"
                        VerticalAlignment="Stretch"
                        CornerRadius="12">
                        <CheckBox
                            VerticalAlignment="Center"
                            FontWeight="Bold"
                            IsChecked="{Binding ShowMemberButton}" />
                    </Border>

                    <TextBlock
                        Grid.Row="5"
                        Grid.Column="0"
                        Margin="25,0,0,0"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Center"
                        Panel.ZIndex="1000"
                        FontWeight="Bold"
                        Foreground="White"
                        Text="Select Printer" />
                    <Border
                        Grid.Row="5"
                        Grid.Column="0"
                        Grid.ColumnSpan="2"
                        Margin="0,5,0,0"
                        Background="#8C97C4"
                        BorderBrush="#8C97C4"
                        BorderThickness="1"
                        CornerRadius="9" />
                    <Border
                        Grid.Row="5"
                        Grid.Column="1"
                        Margin="2,15,10,10"
                        HorizontalAlignment="Stretch"
                        VerticalAlignment="Stretch"
                        CornerRadius="12">
                        <ComboBox
                            HorizontalAlignment="Stretch"
                            ItemsSource="{Binding PrinterNameList}"
                            SelectedItem="{Binding PrinterName}" />
                    </Border>
                </Grid>
            </TabItem>

            <TabItem
                Width="120"
                Height="40"
                Header="Nets">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="0.6*" />
                        <RowDefinition Height="0.2*" />
                    </Grid.RowDefinitions>

                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition />
                            <ColumnDefinition />
                        </Grid.ColumnDefinitions>

                        <Grid.RowDefinitions>
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                        </Grid.RowDefinitions>

                        <TextBlock
                            Grid.Row="0"
                            Grid.Column="0"
                            Margin="25,0,0,0"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Center"
                            Panel.ZIndex="1000"
                            FontWeight="Bold"
                            Foreground="White"
                            Text="NETS Port" />
                        <Border
                            Grid.Row="0"
                            Grid.Column="0"
                            Grid.ColumnSpan="2"
                            Margin="0,5,0,0"
                            Background="#8C97C4"
                            BorderBrush="#8C97C4"
                            BorderThickness="1"
                            CornerRadius="9" />
                        <Border
                            Grid.Row="0"
                            Grid.Column="1"
                            Margin="2,15,10,10"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            CornerRadius="12">
                            <TextBox
                                VerticalContentAlignment="Center"
                                Background="White"
                                BorderThickness="0"
                                Foreground="Black"
                                Text="{Binding NetsIP, UpdateSourceTrigger=PropertyChanged}">
                                <!--<i:Interaction.Behaviors>
                                    <behaviours:IntegerInputBehaviour />
                                </i:Interaction.Behaviors>-->
                            </TextBox>
                        </Border>

                        <TextBlock
                            Grid.Row="1"
                            Grid.Column="0"
                            Margin="25,0,0,0"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Center"
                            Panel.ZIndex="1000"
                            FontWeight="Bold"
                            Foreground="White"
                            Text="NETS QR Host Id" />
                        <Border
                            Grid.Row="1"
                            Grid.Column="0"
                            Grid.ColumnSpan="2"
                            Margin="0,5,0,0"
                            Background="#8C97C4"
                            BorderBrush="#8C97C4"
                            BorderThickness="1"
                            CornerRadius="9" />
                        <Border
                            Grid.Row="1"
                            Grid.Column="1"
                            Margin="2,15,10,10"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            CornerRadius="12">
                            <TextBox
                                VerticalContentAlignment="Center"
                                Background="White"
                                BorderThickness="0"
                                Foreground="Black"
                                Text="{Binding NetsQrHostId, UpdateSourceTrigger=PropertyChanged}">
                                <!--<i:Interaction.Behaviors>
                                    <behaviours:IntegerInputBehaviour />
                                </i:Interaction.Behaviors>-->
                            </TextBox>
                        </Border>

                        <TextBlock
                            Grid.Row="2"
                            Grid.Column="0"
                            Margin="25,0,0,0"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Center"
                            Panel.ZIndex="1000"
                            FontWeight="Bold"
                            Foreground="White"
                            Text="NETS QR Host MId" />
                        <Border
                            Grid.Row="2"
                            Grid.Column="0"
                            Grid.ColumnSpan="2"
                            Margin="0,5,0,0"
                            Background="#8C97C4"
                            BorderBrush="#8C97C4"
                            BorderThickness="1"
                            CornerRadius="9" />
                        <Border
                            Grid.Row="2"
                            Grid.Column="2"
                            Margin="2,15,10,10"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            CornerRadius="12">
                            <TextBox
                                VerticalContentAlignment="Center"
                                Background="White"
                                BorderThickness="0"
                                Foreground="Black"
                                Text="{Binding NetsQrHostMId, UpdateSourceTrigger=PropertyChanged}">
                                <!--<i:Interaction.Behaviors>
                                    <behaviours:IntegerInputBehaviour />
                                </i:Interaction.Behaviors>-->
                            </TextBox>
                        </Border>

                        <TextBlock
                            Grid.Row="3"
                            Grid.Column="0"
                            Margin="25,0,0,0"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Center"
                            Panel.ZIndex="1000"
                            FontWeight="Bold"
                            Foreground="White"
                            Text="NETS QR Gateway Token" />
                        <Border
                            Grid.Row="3"
                            Grid.Column="0"
                            Grid.ColumnSpan="2"
                            Margin="0,5,0,0"
                            Background="#8C97C4"
                            BorderBrush="#8C97C4"
                            BorderThickness="1"
                            CornerRadius="9" />
                        <Border
                            Grid.Row="3"
                            Grid.Column="2"
                            Margin="2,15,10,10"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            CornerRadius="12">
                            <TextBox
                                VerticalContentAlignment="Center"
                                Background="White"
                                BorderThickness="0"
                                Foreground="Black"
                                Text="{Binding NetsQrGatewaytoken, UpdateSourceTrigger=PropertyChanged}">
                                <!--<i:Interaction.Behaviors>
                                    <behaviours:IntegerInputBehaviour />
                                </i:Interaction.Behaviors>-->
                            </TextBox>
                        </Border>

                        <TextBlock
                            Grid.Row="4"
                            Grid.Column="0"
                            Margin="25,0,0,0"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Center"
                            Panel.ZIndex="1000"
                            FontWeight="Bold"
                            Foreground="White"
                            Text="NETS QR Timer (in Minutes)" />
                        <Border
                            Grid.Row="4"
                            Grid.Column="0"
                            Grid.ColumnSpan="2"
                            Margin="0,5,0,0"
                            Background="#8C97C4"
                            BorderBrush="#8C97C4"
                            BorderThickness="1"
                            CornerRadius="9" />
                        <Border
                            Grid.Row="4"
                            Grid.Column="2"
                            Margin="2,15,10,10"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            CornerRadius="12">
                            <TextBox
                                VerticalContentAlignment="Center"
                                Background="White"
                                BorderThickness="0"
                                Foreground="Black"
                                MaxLength="1"
                                Text="{Binding NetsQRTimer, UpdateSourceTrigger=PropertyChanged}">
                                <i:Interaction.Behaviors>
                                    <behaviours:IntegerInputBehaviour />
                                </i:Interaction.Behaviors>
                            </TextBox>
                        </Border>
                    </Grid>
                </Grid>

            </TabItem>
        </TabControl>

        <Grid Grid.Row="2">
            <Grid.ColumnDefinitions>
                <ColumnDefinition />
                <ColumnDefinition />
            </Grid.ColumnDefinitions>

            <Border
                Grid.Column="0"
                Height="60"
                Margin="5"
                HorizontalAlignment="Stretch"
                VerticalAlignment="Center"
                Background="#00B5CB"
                CornerRadius="8"
                Cursor="Hand"
                Focusable="True">
                <i:Interaction.Triggers>
                    <i:EventTrigger EventName="PreviewMouseLeftButtonUp">
                        <i:InvokeCommandAction Command="{Binding CancelCommand}" />
                    </i:EventTrigger>
                </i:Interaction.Triggers>
                <TextBlock
                    HorizontalAlignment="Center"
                    VerticalAlignment="Center"
                    Foreground="White"
                    Text="Cancel" />
            </Border>

            <Border
                Grid.Column="1"
                Height="60"
                Margin="5"
                HorizontalAlignment="Stretch"
                VerticalAlignment="Center"
                Background="#00B5CB"
                CornerRadius="8"
                Cursor="Hand"
                Focusable="True">
                <i:Interaction.Triggers>
                    <i:EventTrigger EventName="PreviewMouseLeftButtonUp">
                        <i:InvokeCommandAction Command="{Binding AddOrUpdateTenantCommand}" />
                    </i:EventTrigger>
                </i:Interaction.Triggers>
                <TextBlock
                    HorizontalAlignment="Center"
                    VerticalAlignment="Center"
                    Foreground="White"
                    Text="Save" />
            </Border>
        </Grid>

    </Grid>


</baseControls:BasePage>
